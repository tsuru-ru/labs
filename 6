''' Задание 1'''
a, b = 'a', 'b'
a, b = b, a
print('1.', a, b)

'''Задание 2'''
a, b, c = 'a', 'b', 'c'
a, b, c = b, c, a
print('2.', a, b, c)

'''Задание 3'''
a, b, c = b, c, a
print('3.', a, b, c)

'''Задание 4'''
x = 2
print('4.', 3 * x ** 6 - 6 * x ** 2 - 7)

'''Задание 5'''
x = 2
x1 = x - 3
print('5.', 4 * x1 ** 6 - 7 * x1 ** 3 + 2)

'''ЗАдание 6'''
a = 4
k = a * a
k *= k
k *= k
print('6.', k)

'''Задание 7'''
k1 = a * a
k2 = k1 * a
k1 *= k2
k2 = k1 * k1
print('7.', k2 * k1)
